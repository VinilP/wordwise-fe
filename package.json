{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "vitest run --coverage && open coverage/index.html",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --reporter=verbose src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run --reporter=verbose src/test/integration/**/*.test.{ts,tsx}",
    "test:accessibility": "vitest run --reporter=verbose src/test/accessibility.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:accessibility",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=test-results.xml",
    "test:visual": "playwright test --project=visual-tests",
    "test:visual:ui": "playwright test --project=visual-tests --ui",
    "test:e2e": "playwright test --project=e2e-tests",
    "test:e2e:ui": "playwright test --project=e2e-tests --ui",
    "test:e2e:cross-browser": "playwright test --project=cross-browser",
    "test:e2e:mobile": "playwright test --project=mobile",
    "test:performance": "playwright test src/test/visual/performance.spec.ts",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:accessibility && npm run test:e2e",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "deploy:dry-run": "echo 'Dry run mode - would deploy to staging' && npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix --audit-level=moderate",
    "security:check": "npx audit-ci --config .audit-ci.json",
    "performance:lighthouse": "npx lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "performance:budget": "npm run build && npx bundlesize",
    "ci:lint": "npm run lint && npm run format:check && npm run type-check",
    "ci:test": "npm run test:all && npm run test:coverage",
    "ci:build": "npm run build && npm run security:check",
    "ci:deploy": "npm run ci:lint && npm run ci:test && npm run ci:build",
    "health:check": "curl -f http://localhost:4173 || exit 1",
    "clean": "rm -rf dist coverage test-results node_modules/.cache",
    "fresh-install": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.86.0",
    "axios": "^1.11.0",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.11.1",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}